{
  "name": "lk_architecture_utils",
  "version": "3.0.0",
  "description": "Utility Helper Package",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "browser": "dist/index.bundle.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c",
    "lint": "eslint --fix 'src/**/*.ts'",
    "test": "jest",
    "publish:public": "npm publish --access public",
    "commit": "cz"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulteja-dev/architecture-utils.git"
  },
  "keywords": [],
  "author": "Rahul Teja A",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rahulteja-dev/architecture-utils/issues"
  },
  "homepage": "https://github.com/rahulteja-dev/architecture-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.5",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test/**/*.test.ts"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
